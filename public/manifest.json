{
    "name": "Protextify - Platform Deteksi Plagiarisme",
    "short_name": "Protextify",
    "description": "Platform deteksi plagiarisme dengan teknologi AI terdepan untuk institusi pendidikan. Menulis tugas, deteksi plagiarisme, dan manajemen kelas yang mudah.",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#23407a",
    "orientation": "portrait-primary",
    "scope": "/",
    "lang": "id-ID",
    "dir": "ltr",
